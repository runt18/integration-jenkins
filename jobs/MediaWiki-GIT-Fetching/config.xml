<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>Collector-job for jobs running when changesets are pushed to Gerrit for review in `mediawiki/core`</description>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>true</disabled>
  <blockBuildWhenDownstreamBuilding>true</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <jdk>(Default)</jdk>
  <triggers class="vector"/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command># Shell script replacing Jenkins git plugin
# Borrowed from OpenStack
/var/lib/jenkins/bin/fetch_gerrit_head.sh</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Ant>
      <targets>cleanextensions take-fingerprints</targets>
      <buildFile>../build.xml</buildFile>
    </hudson.tasks.Ant>
    <hudson.plugins.createfingerprint.CreateFingerprint>
      <targets>fingerprints/*</targets>
    </hudson.plugins.createfingerprint.CreateFingerprint>
    <hudson.plugins.createfingerprint.CreateFingerprint>
      <targets>FETCH_HEAD</targets>
    </hudson.plugins.createfingerprint.CreateFingerprint>
    <hudson.tasks.Shell>
      <command>if [ &quot;$GERRIT_BRANCH&quot; = &quot;master&quot; ]; then /var/lib/jenkins/bin/grunt --gruntfile /var/lib/jenkins/jobs/_shared/gruntfile.js --cwd &quot;$PWD&quot; jshint; else echo &quot;Skipping grunt for this branch&quot;; fi</command>
    </hudson.tasks.Shell>
    <hudson.plugins.parameterizedtrigger.TriggerBuilder>
      <configs>
        <hudson.plugins.parameterizedtrigger.BlockableBuildTriggerConfig>
          <configs class="java.util.Collections$EmptyList"/>
          <projects>MediaWiki-Tests-Databaseless</projects>
          <condition>ALWAYS</condition>
          <triggerWithNoParameters>false</triggerWithNoParameters>
          <block>
            <buildStepFailureThreshold>
              <name>FAILURE</name>
              <ordinal>2</ordinal>
              <color>RED</color>
            </buildStepFailureThreshold>
            <unstableThreshold>
              <name>UNSTABLE</name>
              <ordinal>1</ordinal>
              <color>YELLOW</color>
            </unstableThreshold>
            <failureThreshold>
              <name>FAILURE</name>
              <ordinal>2</ordinal>
              <color>RED</color>
            </failureThreshold>
          </block>
          <buildAllNodesWithLabel>false</buildAllNodesWithLabel>
        </hudson.plugins.parameterizedtrigger.BlockableBuildTriggerConfig>
        <hudson.plugins.parameterizedtrigger.BlockableBuildTriggerConfig>
          <configs class="java.util.Collections$EmptyList" reference="../../hudson.plugins.parameterizedtrigger.BlockableBuildTriggerConfig/configs"/>
          <projects>MediaWiki-Tests-Misc</projects>
          <condition>ALWAYS</condition>
          <triggerWithNoParameters>false</triggerWithNoParameters>
          <block>
            <buildStepFailureThreshold>
